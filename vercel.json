{
  "routes": [
    {
      "src": "/assets/(.*)\\.js$",
      "headers": { "content-type": "application/javascript" },
      "continue": true
    },
    {
      "src": "/(.*)\\.js$",
      "headers": { "content-type": "application/javascript" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}